{
  "name": "elite fire",
  "private": true,
  "description": "Автоматические биокамины Elite Fire",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.7",
    "del": "^6.0.0",
    "gulp": "4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-terser": "^2.1.0",
    "postcss": "8.3.11",
    "postcss-csso": "^6.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.11"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "lint": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp",
    "deploy": "gulp build && gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module",
  "dependencies": {
    "gh-pages": "^3.2.3"
  },
  "version": "1.0.0",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meatrue/elite-fire.git"
  },
  "author": "Трухин Михаил",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meatrue/elite-fire/issues"
  },
  "homepage": "https://github.com/meatrue/elite-fire#readme"
}
